{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "POSTGRES_URL": "@postgres_url",
    "SITEMAP_URL": "@sitemap_url",
    "COHERE_API_KEY": "@cohere_api_key",
    "COLLECTION_NAME": "@collection_name",
    "DOCUSAURUS_DOMAIN": "@docusaurus_domain"
  }
}